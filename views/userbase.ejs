<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vehicle Detection</title>
    <link rel="stylesheet" href="css/userbase.css">
</head>

<body>

    <div class="parent">
        <div class="div1">
            <div class="header">
                <h1>Vehicle Detection</h1>
                <p>Powered by image processing</p>
            </div>
        </div>
        <div class="div2">
            <div class="container container1">
                <div class="row ">
                    <div class="column left ">
                        <span class="dot" style="background:#ED594A;"></span>
                        <span class="dot" style="background:#FDD800;"></span>
                        <span class="dot" style="background:#5AC05A;"></span>
                    </div>
                    <div class="column middle">
                        <input type="text" value="Preprocessing" disabled>
                    </div>
                </div>

                <div class="content">
                    <div class="box">
                        <!-- <h3>load Vehicle video</h3> -->
                        <!-- <p>How to create a detailed browser window look with CSS.</p> -->
                        <img class="images" src="images/processingGif.gif" alt="loading vehicle ">
                    </div>
                </div>
            </div>
        </div>
        <div class="div3">
            <!-- <h1>postprocessing image</h1>
            <div class="inputbox">
                output image
            </div> -->
            <div class="container container2">
                <div class="row">
                    <div class="column left">
                        <span class="dot" style="background:#ED594A;"></span>
                        <span class="dot" style="background:#FDD800;"></span>
                        <span class="dot" style="background:#5AC05A;"></span>
                    </div>
                    <div class="column middle">
                        <input type="text" value="Postprocessing image " disabled>
                    </div>

                </div>

                <div class="content">
                    <div class="box">
                        <!-- <div class="sampleimput-box"> -->

                        <img class="images" src="images/carCapture.png" alt="">
                        <!-- </div> -->
                        <!-- <div class="inputSampleRes">hello</div> -->
                    </div>

                </div>
            </div>
        </div>
        <div class="div4">
            <div class="container container3">
                <div class="row">
                    <div class="column left">
                        <span class="dot" style="background:#ED594A;"></span>
                        <span class="dot" style="background:#FDD800;"></span>
                        <span class="dot" style="background:#5AC05A;"></span>
                    </div>
                    <div class="column middle">
                        <h3>Result</h3>
                    </div>

                </div>

                <div class="content">
                    <div class="box">
                        <!-- <ul>
                            <li>speed</li>
                            <li>location</li>
                            <li>
                                details send to owner
                            </li>
                        </ul> -->


                        <% data.forEach(item=> { %>
                            <ul>
                                <li>ID: <%= item.id %>, </li>
                                <li>Speed: <%= item.speed %>,</li>
                                <li>Created at: <%= item.created_at %>
                                </li>
                            </ul>
                            <% }); %>



                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>